---
import { Image } from 'astro:assets'
import Layout from '../layouts/Layout.astro'
import Card from '../components/Card.astro'
import zonweringScreenshot from '../assets/zonwering_screenshot.webp'
import sb from '../assets/sb.webp'
import brainnovation from '../assets/brainnovation.webp'
import valleibloemen from '../assets/valleibloemen.webp'
import portfolioScreenshot from '../assets/portfolio_screenshot.webp'
import guust from '../assets/guust.webp'
import heartfelt from '../assets/Heartfelt.webp'
---

<Layout title="Projecten - Evi Wammes" description="Bekijk hier mijn afgelopen projectend die ik heb gedaan.">
	<main class="grid-rows-auto mt-4 gap-3 space-y-5 lg:space-y-0 text-center sm:grid sm:grid-flow-col sm:auto-rows-auto grid-cols-1 lg:grid-cols-2 w-4/5 lg:w-4/5 2xl:w-8/12">
		<h1 class="col-span-3 row-span-1 text-5xl font-bold text-stone-800 dark:text-stone-100 mb-3">Projecten</h1>
		<article class="self-start col-start-1 row-start-2 grid h-fit auto-rows-auto grid-cols-3 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">Wordpress</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-self-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Ontwerp & Bouw</span>
			<span class="row-start-1 col-start-3 w-fit self-center justify-self-end text-right text-xs lg:text-smopacity-70"><a href="https://zonweringapeldoorn.nl" target="_blank">Zonweringapeldoorn.nl</a></span>
			<h2 class="col-span-full row-start-2 h-10 w-fit text-left text-2xl sm:text-3xl">Zonwering Apeldoorn</h2>
			<Image src={zonweringScreenshot} alt="Laptop met de website van Zonwering" class="col-span-3 row-start-3 w-full rounded-2xl" />
			<a class="row-start-4 col-span-2 lg:col-span-1 rounded-xl bg-cyan-50 px-4 py-1 text-base text-black hover:bg-sky-700 hover:text-white hover:scale-95 dark:bg-sky-800 dark:text-white" href="projecten/zonwering-apeldoorn">
				Lees meer
			</a>
		</article>

		<article class="self-start col-span-2 row-start-2 grid h-fit auto-rows-auto grid-cols-3 gap-x-0.5 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="col-start-1 row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">Wordpress</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-self-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Development</span>
			<span class="col-start-3 row-start-1 w-fit self-center justify-self-end text-right opacity-70 text-xs lg:text-sm"><a href="https://saucijzenbakker.nl/" target="_blank">saucijzenbakker.nl</a></span>
			<h2 class="col-span-full row-start-2 h-10 w-fit text-left text-2xl sm:text-3xl">Saucijzenbakker</h2>
			<Image src={sb} alt="Laptop met de website van de Saucijzenbakker" class="col-span-3 row-start-3 w-full rounded-2xl" />
			<span class="col-span-full row-start-4 w-fit rounded-xl bg-cyan-50 px-4 py-1 text-base text-black hover:bg-sky-700 hover:text-white dark:bg-sky-800 dark:text-white"> Enkel het chauffeur order-systeem is door mij gemaakt.</span>
		</article>

		<article class="self-start col-start-1 row-span-3 grid h-fit auto-rows-auto grid-cols-3 gap-x-0.5 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="col-start-1 row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">Wordpress</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-self-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Ontwerp & Bouw</span>
			<span class="col-start-3 row-start-1 w-fit self-center justify-self-end text-right opacity-70 text-xs lg:text-sm"><a href="https://brainnovation.nl" target="">Brainnovation.nl</a></span>
				<h2 class=" col-span-full row-start-2 h-10 w-fit text-left text-2xl sm:text-3xl">Brainnovation</h2>
				<Image src={brainnovation} alt="Laptop met de website van Brainnovation" class="col-span-3 row-start-3 w-full rounded-2xl" />
				<a class="row-start-4 col-span-2 lg:col-span-1 rounded-xl bg-cyan-50 px-4 py-1 text-base text-black hover:bg-sky-700 hover:text-white hover:scale-95 dark:bg-sky-800 dark:text-white" href="projecten/brainnovation">
					Lees meer
				</a>
		</article>

		<article class="self-start col-span-2 row-span-3 grid h-fit auto-rows-auto grid-cols-3 gap-x-0.5 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="col-start-1 row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">Wordpress</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-self-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Development</span>
			<span class="col-start-3 row-start-1 w-fit self-center justify-self-end text-right  opacity-70 text-xs lg:text-sm"><a href="https://valleibloemen.nl" target="">Valleibloemen.nl</a></span>
			<h2 class="col-span-full row-start-2 h-10 w-fit text-left text-2xl sm:text-3xl">Valleibloemen</h2>
			<Image src={valleibloemen} alt="Laptop met de website van Valleibloemen" class="col-span-3 row-start-3 w-full rounded-2xl" />
		</article>

		<article class="self-start col-start-1 row-span-4 grid h-fit auto-rows-auto grid-cols-3 gap-x-0.5 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="col-start-1 row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">VueJS</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Nuxt3 & TailwindCSS</span>
			<span class="col-start-3 row-start-1 w-fit self-center justify-self-end text-right  opacity-70 text-xs lg:text-sm"><nuxt-link to="/">Eviwammes.amsterdam</nuxt-link></span>
			<h2 class="col-span-full h-10 text-left text-2xl sm:text-3xl">Portfolio</h2>
			<Image src={portfolioScreenshot} alt="Laptop met de website van Evi Wammes" class="col-span-3 row-start-3 w-full rounded-2xl" />
			<a class="row-start-4 col-span-2 lg:col-span-1 rounded-xl bg-cyan-50 px-4 py-1 text-base text-black hover:bg-sky-700 hover:text-white hover:scale-95 dark:bg-sky-800 dark:text-white" href="projecten/portfolio">
				Lees meer
			</a>
		</article>

		<article class="self-start col-span-2 row-span-4 grid h-fit auto-rows-auto grid-cols-3 gap-x-0.5 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="col-start-1 row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">Wordpress</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-self-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Ontwerp & Bouw</span>
			<span class="col-start-3 row-start-1 w-fit self-center justify-self-end text-right  opacity-70 text-xs lg:text-sm"><a href="https://guust-meubelmakerij.nl" target="">Guust-meubelmakerij.nl</a></span>
			<h2 class="col-span-full h-10 text-left text-2xl sm:text-3xl">Guust Meubelmakerij</h2>
			<Image src={guust} alt="Laptop met de website van Guust Meubelmakerij" class="col-span-3 row-start-3 w-full rounded-2xl" />
			<a class="row-start-4 col-span-2 lg:col-span-1 rounded-xl bg-cyan-50 px-4 py-1 text-base text-black hover:bg-sky-700 hover:text-white hover:scale-95 dark:bg-sky-800 dark:text-white" href="projecten/guust-meubelmakerij">
				Lees meer
			</a>
		</article>

		<article class="self-start col-start-1 row-span-5 grid h-fit auto-rows-auto grid-cols-3 gap-x-0.5 gap-y-2 rounded-xl bg-sky-500 p-6 text-white hover:shadow-xl dark:bg-slate-800 dark:hover:shadow-[0_20px_50px_-5px_rgba(255,255,255,0.3)]">
			<span class="col-start-1 row-start-1 w-fit self-center rounded-full bg-white px-2 py-1 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:bg-cyan-700 dark:text-white dark:text-white dark:opacity-100 dark:opacity-100">Wordpress</span>
			<span class="hidden lg:block col-start-2 row-start-1 w-fit self-center justify-self-center rounded-full bg-white p-1 px-2 text-left text-sm text-black opacity-80 dark:bg-cyan-700 dark:text-white dark:opacity-100">Ontwerp & Bouw</span>
			<span class="col-start-3 row-start-1 w-fit self-center justify-self-end text-right text-xs lg:text-sm opacity-70"><a href="https://heartfeltequinebodywork.nl/" target="">Heartfeltequinebodywork.nl</a></span>
			<h2 class="col-span-full h-10 text-left text-[1.4rem] sm:text-3xl">Heartfelt Equine Bodywork</h2>
			<Image src={heartfelt} alt="Laptop met de website van Heartfelt Equine Bodywork" class="col-span-3 row-start-3 w-full rounded-2xl" />
			<a class="row-start-4 col-span-2 lg:col-span-1 rounded-xl bg-cyan-50 px-4 py-1 text-base text-black hover:bg-sky-700 hover:text-white hover:scale-95 dark:bg-sky-800 dark:text-white" href="projecten/heartfelt">
				Lees meer
			</a>
		</article>
	</main>
</Layout>
